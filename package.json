{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "basic react js",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --progress --hot",
    "build-prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "react"
  ],
  "author": "Rahul Anand",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
